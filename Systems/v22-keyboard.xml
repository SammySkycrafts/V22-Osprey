<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <key n="68">
    <name>D</name>
    <desc>Cargo doors</desc>
    <binding>
      <command>nasal</command>
      <script>doors.passenger.toggle();</script>
    </binding>
  </key>

  <key n="100">
    <name>d</name>
    <desc>Crew doors</desc>
    <binding>
      <command>nasal</command>
      <script>doors.crew.toggle();</script>
    </binding>
  </key>

  <key n="78">
    <name>N</name>
    <desc>Fold wing back</desc>
  </key>

  <key n="110">
    <name>n</name>
    <desc>Unfold wing</desc>
  </key>

  <key n="123">
    <name>{</name>
    <desc>Shutdown Engines</desc>
    <binding>
      <command>nasal</command>
      <script>v22.engines(0)</script>
    </binding>
  </key>

  <key n="125">
    <name>}</name>
    <desc>Start Engines</desc>
    <binding>
      <command>nasal</command>
      <script>v22.engines(1)</script>
    </binding>
  </key>

  <key n="109">
    <name>m</name>
    <desc>Tilt Rotors fwd</desc>
    <repeatable>false</repeatable>
    <mod-alt>
      <binding>
        <command>nasal</command>
        <script>v22.set_tilt(0,0)</script>
      </binding>
    </mod-alt>
  </key>

  <key n="77">
    <name>M</name>
    <desc>Tilt Rotors aft</desc>
    <repeatable>false</repeatable>
  </key>

</PropertyList>
