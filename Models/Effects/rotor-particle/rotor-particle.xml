polation>          <entry><ind>  0 </ind><dep> 1.00 </dep></entry>          <entry><ind>  2 </ind><dep> 1.40 </dep></entry>          <entry><ind> 11 </ind><dep> 2.57 </dep></entry>          <entry><ind> 25 </ind><dep> 3.27 </dep></entry>        </interpolation>      </life-sec>      <mass-kg>0.035</mass-kg>      <radius-m>0.045</radius-m>    </particle>    <program>      <fluid>air</fluid>      <gravity>false</gravity>      <wind>true</wind>    </program>  </particlesystem>    <!-- == particles - right rotor DIRT == -->  <animation>    <name>dirt_right</name>      <object-name>dirt_right_01</object-name>      <object-name>dirt_right_02</object-name>  </animation>    <animation>    <type>rotate</type>    <object-name>dirt_right</object-name>    <property>orientation/pitch-deg</property>    <axis>      <x>  0 </x>      <y> -1 </y>      <z>  0 </z>    </axis>  </animation>  <animation>    <type>rotate</type>    <object-name>dirt_right</object-name>    <property>orientation/roll-deg</property>    <axis>      <x> 1 </x>      <y> 0 </y>      <z> 0 </z>    </axis>  </animation>  <animation>    <type>translate</type>    <object-name>dirt_right</object-name>    <!-- <property>position/gear-agl-m</property> -->    <property>sim/multiplay/generic/float[6]</property>    <axis>      <x>  0 </x>      <y>  0 </y>      <z> -1 </z>    </axis>  </animation>  <!-- right rotor DIRT - move particle system with engine tilt -->  <animation>    <type>translate</type>    <object-name>dirt_right</object-name>    <!-- sim/model/v22/animation_tilt -->    <property>sim/multiplay/generic/float[12]</property>    <factor>0.05</factor>    <axis>      <x> 1 </x>      <y> 0 </y>      <z> 0 </z>    </axis>  </animation>    <!-- left rotor DIRT - roate particles experimental -->  <animation>    <type>rotate</type>    <object-name>dirt_right_01</object-name>    <!-- rotors/main/blade[0]/position-deg -->    <property>sim/multip