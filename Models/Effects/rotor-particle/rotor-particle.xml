<?xml version="1.0" encoding="ISO-8859-1"?><PropertyList>  <!-- ==                                     == -->  <!-- == ROTOR LEFT & RIGHT PARTICLE SYSTEMS == -->  <!-- ==                                     == -->    <!-- NORMAL GROUND left rotor DIRT - particle system controled by engines-->  <particlesystem>    <name>dirt_left_01</name>    <texture>smoke_dirt.png</texture>    <emissive>false</emissive>    <lighting>false</lighting>    <offsets>      <x-m> -1.5 </x-m>      <y-m> -6.8 </y-m>      <z-m> -2.9 </z-m>      <roll-deg> 90 </roll-deg>    </offsets>    <attach>world</attach>    <placer>      <type>point</type>    </placer>    <condition>      <not>        <property>sim/paused</property>      </not>      <not>        <property>sim/crashed</property>      </not>      <less-than>        <!-- position/altitude-agl-ft -->        <property>sim/multiplay/generic/float[8]</property>        <value>30</value>      </less-than>      <greater-than>        <property>rotors/tail/rpm</property>        <value>7</value> switched of for test      </greater-than>    </condition>    <shooter>      <theta-min-deg>   0.0 </theta-min-deg>      <theta-max-deg> 360.0 </theta-max-deg>      <phi-min-deg>    -0.5 </phi-min-deg>      <phi-max-deg>    35.2 </phi-max-deg>      <speed-mps>        <!-- rotors/tail/rpm -->        <property>sim/multiplay/generic/float[9]</property>        <factor>1</factor>        <interpolation>          <entry><ind>  0 </ind><dep> 200 </dep></entry>          <entry><ind>  2 </ind><dep> 200 </dep></entry>          <entry><ind> 25 </ind><dep> 600 </dep></entry>        </interpolation>        <spread>75</spread>      </speed-mps>      <rotation-speed>        <x-max-deg-sec>  15 </x-max-deg-sec>        <x-min-deg-sec> -15 </x-min-deg-sec>        <y-max-deg-sec>  15 </y-max-deg-sec>        <y-min-deg-sec> -15 </y-min-deg-sec>        <z-max-deg-sec>  15 </z-max-deg-sec>        <z-min-deg-sec> -15 </z-min-deg-sec>      </rotation-speed>    </shooter>    <counter>      <particles-per-sec>        <!-- position/altitude-agl-ft -->        <property>sim/multiplay/generic/float[8]</property>        <factor>-40</factor>        <offset-m>2000</offset-m>      </particles-per-sec>    </counter>    <align>billboard</align>    <particle>      <start>        <color>          <red><value>   0.95 </value></red>          <green><value> 0.95 </value></green>          <blue><value>  0.95 </value></blue>          <alpha>            <!-- rotors/tail/rpm -->            <property>sim/multiplay/generic/float[9]</property>            <factor>0.00004</factor>            <max>0.02</max>          </alpha>        </color>        <size>          <value>0.5</value>        </size>      </start>      <end>        <color>          <red><value>   1 </value></red>          <green><value> 1 </value></green>          <blue><value>  1 </value></blue>          <alpha>            <!-- rotors/tail/rpm -->            <property>sim/multiplay/generic/float[9]</property>            <factor>0.0002</factor>            <max>0.04</max>          </alpha>        </color>                <size>          <factor>1</factor>          <!-- sim/model/v22/rotor/left/collective -->          <property>sim/multiplay/generic/float[10]</property>          <interpolation>            <entry><ind>  0 </ind><dep> 0.8 </dep></entry>            <entry><ind>  2 </ind><dep> 1.8 </dep></entry>            <entry><ind> 11 </ind><dep> 1.9 </dep></entry>            <entry><ind> 25 </ind><dep> 3.0 </dep></entry>          </interpolation>          <spread>0.4</spread>        </size>      </end>      <life-sec>        <!-- sim/model/v22/rotor/left/collective -->        <property>sim/multiplay/generic/float[10]</property>        <factor>1</factor>        <interpolation>          <entry><ind>  0 </ind><dep> 1.00 </dep></entry>          <entry><ind>  2 </ind><dep> 1.30 </dep></entry>          <entry><ind> 11 </ind><dep> 2.27 </dep></entry>          <entry><ind> 25 </ind><dep> 2.27 </dep></entry>        </interpolation>      </life-sec>      <mass-kg>0.015</mass-kg>      <radius-m>0.045</radius-m>    </particle>    <program>      <fluid>air</fluid>      <gravity>false</gravity>      <wind>true</wind>    </program>  </particlesystem>      <!-- Low GROUND left rotor DIRT - particle system controled by engines-->  <particlesystem>    <name>dirt_left_02</name>    <texture>smoke_dirt.png</texture>    <emissive>false</emissive>    <lighting>false</lighting>    <offsets>      <x-m> -1.5 </x-m>      <y-m> -6.8 </y-m>      <z-m> -2.9 </z-m>      <roll-deg> 90 </roll-deg>    </offsets>    <attach>world</attach>    <placer>      <type>point</type>    </placer>    <condition>      <not>        <property>sim/paused</property>      </not>      <not>        <property>sim/crashed</property>      </not>      <less-than>        <!-- position/altitude-agl-ft -->        <property>sim/multiplay/generic/float[8]</property>        <value>30</value>      </less-than>      <greater-than>        <property>rotors/tail/rpm</property>        <value>7</value>      </greater-than>    </condition>    <shooter>      <theta-min-deg>   0.0 </theta-min-deg>      <theta-max-deg> 360.0 </theta-max-deg>      <phi-min-deg>    -0.5 </phi-min-deg>      <phi-max-deg>     3.2 </phi-max-deg>      <speed-mps>        <!-- rotors/tail/rpm -->        <property>sim/multiplay/generic/float[9]</property>        <factor>1</factor>        <interpolation>          <entry><ind>  0 </ind><dep> 20 </dep></entry>          <entry><ind>  2 </ind><dep> 20 </dep></entry>          <entry><ind> 25 </ind><dep> 30 </dep></entry>        </interpolation>        <spread>50</spread>      </speed-mps>      <rotation-speed>        <x-max-deg-sec>  15 </x-max-deg-sec>        <x-min-deg-sec> -15 </x-min-deg-sec>        <y-max-deg-sec>  15