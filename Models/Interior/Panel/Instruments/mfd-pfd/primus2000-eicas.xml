  <animation>
    <type>rotate</type>
    <object-name>Flapneedle</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 70 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.003 </x-m>
      <y-m> -0.070 </y-m>
      <z-m> -0.042 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

