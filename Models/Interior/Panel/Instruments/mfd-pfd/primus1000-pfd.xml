      <entry><ind>     0 </ind><dep>   0.0 </dep></entry>
      <entry><ind>   500 </ind><dep>   9.5 </dep></entry>
      <entry><ind>  1000 </ind><dep>  19.0 </dep></entry>
      <entry><ind>  2000 </ind><dep>  38.0 </dep></entry>
      <entry><ind>  4000 </ind><dep>  80.0 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.004 </x-m>
      <y-m>  0.081 </y-m>
      <z-m> -0.042 </z-m>
    </center>
    <axis>
