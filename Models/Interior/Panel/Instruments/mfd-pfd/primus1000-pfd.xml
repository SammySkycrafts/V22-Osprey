  <animation>
    <type>translate</type>
    <object-name>CRSneedle1</object-name>
    <property>instrumentation/nav/heading-needle-deflection</property>
    <interpolation>
      <entry><ind> -10 </ind><dep> -0.0148 </dep></entry>
      <entry><ind>   0 </ind><dep>  0.0000 </dep></entry>
      <entry><ind>  10 </ind><dep>  0.0148 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.002 </x-m>
